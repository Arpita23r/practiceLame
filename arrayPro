
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int pro=1;
      int[] arr=new int[n];
      for(int i=0;i<n;i++) {
          arr[i]=sc.nextInt();
      }
      for(int i=0;i<n;i++) {
       pro=pro*arr[i];
      }
    System.out.println(pro);
    }
}
